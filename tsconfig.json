{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "target": "ESNext",
    "plugins": [
      {
        "name": "gql.tada/ts-plugin",
        "schema": "./app/graphql/types/github-schema.graphql",
        "tadaOutputLocation": "./app/graphql/types/github-graphql-env.d.ts",
        "tadaTurboLocation": "./app/graphql/types/github-graphql-cache.d.ts"
      }
    ]
  },
  "files": [],
  "references": [
    { "path": "./.nuxt/tsconfig.app.json" },
    { "path": "./.nuxt/tsconfig.server.json" },
    { "path": "./.nuxt/tsconfig.shared.json" },
    { "path": "./.nuxt/tsconfig.node.json" }
  ]
}
